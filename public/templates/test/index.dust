<script type="text/javascript">

    function netType(netType, callback) {
        var UA = navigator.userAgent;
        //var UA = "Mozilla/5.0 (iPhone; CPU iPhone OS 10_3_3 like Mac OS X) AppleWebKit/603.3.8 (KHTML, like Gecko) Mobile/14G60 MicroMessenger/6.5.12 NetType/WIFI Language/zh_CN";
        if (UA) {
            var netType_arr = UA.match(/NetType.*\s/g);
            if (netType_arr && netType_arr[0]) {
                var _netType = netType_arr[0].replace('NetType/', '');
                _netType = _netType.replace(/\s/g,'')
                if(_netType == netType){
                    callback
                }
            }
        }
    }

    function callback(){
        console.log('fasdf');
    }
    netType('WIFI',callback)


</script>
